<br>
<div class="container">
	<%- if controller_name != 'sessions' %>
		<div class="row">
			<div class="col-md-offset-4 col-md-6">
				
				<%= link_to "Log in", new_session_path(resource_name), class:"" %><br /><br />
			</div>
		</div>
	<% end -%>
	<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
		<div class="row">
			<div class="col-md-offset-4 col-md-6">
				  <%= link_to "Sign up", new_registration_path(resource_name), class: "ui tag label" %><br /><br />
			</div>
		</div>
	<% end -%>
	<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
		<div class="row">
			<div class="col-md-offset-4 col-md-6">
		  		<%= link_to "Forgot your password?" ,new_password_path(resource_name) ,class: "ui red tag label" %><br /><br />
			</div>
		</div>
	<% end -%>
		<div class="row">
			<div class="col-md-offset-4 col-md-6">
				
			</div>
		</div>
</div>
	



	

	<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
	  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name) %><br /><br />
	<% end -%>

	<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
	  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name) %><br /><br />
	<% end -%>

	<%- if devise_mapping.omniauthable? %>
	  <%- resource_class.omniauth_providers.each do |provider| %>
	    <%= link_to "Sign in with #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider) %><br /><br />
	  <% end -%>
	<% end -%>
